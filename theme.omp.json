{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "template": "\n",
          "type": "text",
          "style": "plain"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#34444c",
          "foreground": "#26C6DA",
          "style": "plain",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} \ue0b1",
          "type": "os"
        },
        {
          "background": "#34444c",
          "foreground": "#26C6DA",
          "style": "plain",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "background": "#34444c",
          "foreground": "#26C6DA",
          "properties": {
            "style": "agnoster_short",
            "mapped_locations": {
              "C:\\dev": "dev",
              "C:\\Users\\tobias.johansson_tra\\source\\kit-consulting": "kit"
            }
          },
          "style": "plain",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "background": "#34444c",
          "foreground": "#D4E157",
          "style": "plain",
          "template": "{{ if .Env.POSH_GIT_STRING }} {{ .Env.POSH_GIT_STRING }} {{ end }}",
          "type": "git"
        },
        {
          "background": "transparent",
          "foreground": "#34444c",
          "style": "plain",
          "template": "\ue0b0 ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
